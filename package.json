{
  "name": "crater",
  "scripts": {
    "start": "defaultenv env/dev.js babel-node scripts/start.js",
    "debug": "defaultenv env/dev.js babel-node scripts/debug.js",
    "debug-brk": "defaultenv env/dev.js babel-node scripts/debug-brk.js",
    "build": "defaultenv env/prod.js babel-node scripts/build.js",
    "build:meteor": "defaultenv env/prod.js babel-node scripts/build-meteor.js",
    "build:server": "defaultenv env/prod.js babel-node scripts/build-server.js",
    "build:client": "defaultenv env/prod.js babel-node scripts/build-client.js",
    "prod": "defaultenv env/prod.js babel-node scripts/prod.js",
    "prod:debug": "defaultenv env/prod.js babel-node scripts/prod-debug.js",
    "prod:debug-brk": "defaultenv env/prod.js babel-node scripts/prod-debug-brk.js",
    "test": "BABEL_ENV=test mocha $npm_package_config_mocha",
    "test:hot-reloading": "BABEL_ENV=test mocha $npm_package_config_mocha --grep 'hot reloading'",
    "test:dev": "BABEL_ENV=test mocha $npm_package_config_mocha --grep dev",
    "test:prod": "BABEL_ENV=test mocha $npm_package_config_mocha --grep prod",
    "test:docker": "BABEL_ENV=test mocha $npm_package_config_mocha --grep docker",
    "coverage": "BABEL_ENV=coverage nyc --reporter=lcov --reporter=text mocha $npm_package_config_mocha",
    "build:docker": "defaultenv env/prod.js babel-node scripts/build-docker.js",
    "docker": "defaultenv env/prod.js babel-node scripts/docker.js",
    "flow": "flow",
    "flow:watch": "flow-watch --watch flowlib/ --watch scripts/ --watch src/ --watch test/ --watch webpack/ --watch .flowconfig",
    "lint": "eslint *.js src scripts test util webpack",
    "lint:fix": "eslint --fix *.js src scripts test util webpack",
    "lint:watch": "esw -w *.js src scripts test util webpack",
    "open:coverage": "open coverage/lcov-report/index.html"
  },
  "config": {
    "mocha": "--compilers js:babel-core/register ./test/integration/index.js"
  },
  "dependencies": {
    "babel-plugin-module-resolver": "^2.7.1",
    "bcrypt": "^1.0.1",
    "debug": "^2.5.2",
    "es6-promisify": "^5.0.0",
    "express": "^4.14.0",
    "fibers": "^1.0.15",
    "immutable": "^3.8.1",
    "json-string-loader": "1.0.0",
    "meteor-node-stubs": "^0.2.4",
    "prop-types": "^15.5.10",
    "react": "15.5.4",
    "react-dom": "^15.4.0",
    "react-dom-stream": "^0.5.1",
    "react-redux": "^5.0.0",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.5.2",
    "redux-immutablejs": "0.0.8"
  },
  "version": "2.0.0",
  "main": "index.js",
  "author": "Andy Edwards <jedwards@fastmail.com>",
  "license": "ISC",
  "description": "Meteor(/React) app skeleton that runs your app code with pure Webpack/Babel instead of isobuild",
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "async-child-process": "^1.0.0",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-meteor-imports": "^1.0.3",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-flow": "^1.0.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.13.0",
    "babel-register": "^6.14.0",
    "babel-runtime": "^6.23.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "crater-util": "^1.2.2",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.26.0",
    "defaultenv": "^1.1.0",
    "empty": "^0.10.1",
    "eslint": "^3.10.2",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-flowtype": "^2.17.1",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.7.1",
    "eslint-watch": "^3.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fake-style-loader": "^1.0.1",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.42.0",
    "flow-watch": "^1.0.0",
    "glob": "^7.0.6",
    "happypack": "^3.0.0",
    "http-proxy": "^1.14.0",
    "istanbul": "^0.4.5",
    "json-loader": "^0.5.4",
    "meteor-imports-webpack-plugin": "^1.1.5",
    "mkdirp": "^0.5.1",
    "mocha": "^3.0.2",
    "node-inspector": "^0.12.8",
    "nyc": "^10.0.0",
    "phantomjs-prebuilt": "2.1.4",
    "popsicle": "^9.0.0",
    "postcss-loader": "^1.1.1",
    "postcss-modules-values": "^1.2.2",
    "pre-commit": "^1.2.2",
    "progress-bar-webpack-plugin": "^1.9.0",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^3.0.0-beta.2",
    "redux-logger": "^2.7.4",
    "rimraf": "^2.5.4",
    "smart-restart": "^1.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webdriverio": "^4.4.0",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-node-externals": "^1.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jedwards1211/crater.git"
  },
  "keywords": [
    "meteor",
    "babel",
    "webpack",
    "react",
    "crater"
  ],
  "bugs": {
    "url": "https://github.com/jedwards1211/crater/issues"
  },
  "homepage": "https://github.com/jedwards1211/crater#readme",
  "nyc": {
    "include": [
      "src/**/*.js",
      "scripts/**/*.js"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "pre-commit": [
    "lint",
    "flow"
  ]
}
